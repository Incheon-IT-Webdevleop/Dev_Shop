<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.shop.inquery.InQueryMapper">

    <!-- 문의사항 목록조회 쿼리 (최신글 순) -->
    <select id="findAllInquery" resultType="com.shop.shop.inquery.InqueryDto">
        SELECT
        inqueryIdx,
        category,
        content,
        createAt,
        orderIdx,
        productIdx,
        answer,
        answerAt
        FROM Inquery
        ORDER BY createAt DESC;
    </select>
</mapper>